<resources>
  <!-- We don't want to publish private symbols in android.R as part of the
       SDK.  Instead, put them here. -->
  <private-symbols package="com.android.internal" />

  <!-- Private symbols that we need to reference from framework code.  See
       frameworks/base/core/res/MakeJavaSymbols.sed for how to easily generate
       this.
  -->

  <!-- Reboot - Advanced Reboot -->
  <java-symbol type="string" name="reboot_confirm" />
  <java-symbol type="string" name="reboot_title" />
  <java-symbol type="string" name="reboot_progress" />
  <java-symbol type="string" name="global_action_reboot" />
  <java-symbol type="drawable" name="ic_lock_power_reboot" />
  <java-symbol type="array" name="shutdown_reboot_options" />
  <java-symbol type="array" name="shutdown_reboot_actions" />
  <java-symbol type="bool" name="config_advanced_reboot" />

  <!-- kill app long press back -->
  <java-symbol type="string" name="app_killed_message" />
  <java-symbol type="integer" name="config_backKillTimeout" />

  <java-symbol type="bool" name="config_ledCanPulse" /> 

  <!-- Power menu -->
  <java-symbol type="drawable" name="ic_lock_screenshot" />
  <java-symbol type="string" name="global_action_screenshot" />
  <java-symbol type="string" name="global_action_search" />
  <java-symbol type="string" name="global_action_voicesearch" />
  <java-symbol type="drawable" name="ic_lock_settings" />
  <java-symbol type="drawable" name="ic_lock_user" />
  <java-symbol type="drawable" name="ic_lock_assist" />
  <java-symbol type="drawable" name="ic_lock_voice" />

  <!-- Slim recent app screen-->
  <java-symbol type="integer" name="config_recentDefaultDur" />
  <java-symbol type="integer" name="config_recentExitDur" />
  <java-symbol type="style" name="Animation.RecentScreen" />
  <java-symbol type="style" name="Animation.RecentScreen.Left" />
  <java-symbol type="anim" name="recent_screen_enter" />
  <java-symbol type="anim" name="recent_screen_enter_left" />
  <java-symbol type="anim" name="recent_screen_fade_out" />

  <!-- Config IME switcher -->
  <java-symbol type="bool" name="config_show_IMESwitcher" />

  <!-- Shortcut actions -->
  <java-symbol type="drawable" name="ic_shortcut_action_silent" />
  <java-symbol type="drawable" name="ic_shortcut_action_theme_switch" />
  <java-symbol type="drawable" name="ic_shortcut_action_torch" />
  <java-symbol type="drawable" name="ic_shortcut_action_vib" />
  <java-symbol type="drawable" name="ic_shortcut_action_back" />
  <java-symbol type="drawable" name="ic_shortcut_action_back_ime" />
  <java-symbol type="drawable" name="ic_shortcut_action_camera" />
  <java-symbol type="drawable" name="ic_shortcut_action_expanded_desktop" />
  <java-symbol type="drawable" name="ic_shortcut_action_home" />
  <java-symbol type="drawable" name="ic_shortcut_action_ime_switcher" />
  <java-symbol type="drawable" name="ic_shortcut_action_killtask" />
  <java-symbol type="drawable" name="ic_shortcut_action_lastapp" />
  <java-symbol type="drawable" name="ic_shortcut_action_menu_big" />
  <java-symbol type="drawable" name="ic_shortcut_action_navbar" />
  <java-symbol type="drawable" name="ic_shortcut_action_notifications" />
  <java-symbol type="drawable" name="ic_shortcut_action_null" />
  <java-symbol type="drawable" name="ic_shortcut_action_pie" />
  <java-symbol type="drawable" name="ic_shortcut_action_power" />
  <java-symbol type="drawable" name="ic_shortcut_action_power_menu" />
  <java-symbol type="drawable" name="ic_shortcut_action_quicksettings" />
  <java-symbol type="drawable" name="ic_shortcut_action_recent" />
  <java-symbol type="drawable" name="ic_shortcut_action_ring_vib_silent" />
  <java-symbol type="drawable" name="ic_shortcut_action_screenshot" />
  <java-symbol type="drawable" name="ic_shortcut_action_search" />

  <!-- BatteryLED fast charge -->
  <java-symbol type="integer" name="config_chargingFastThreshold" />

</resources>
